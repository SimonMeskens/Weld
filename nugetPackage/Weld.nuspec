<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Weld</id>
        <version>1.1.0</version>
        <title>Weld TypeScript MVC</title>
        <authors>Martijn Muurman</authors>
        <owners />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Weld is a open-source framework for creating TypeScript classes based on your MVC controller. Weld makes it easier to use ajax calls in your TypeScipt.</description>
        <summary>Generates TypeScript classes on your MVC controller.</summary>
        <releaseNotes>Added support for
* ViewModels to Typescript
* ClientUrl</releaseNotes>
        <copyright>2013</copyright>
        <language>en-US</language>
        <tags>typescipt mvc</tags>
        <dependencies>
            <dependency id="jquery.TypeScript.DefinitelyTyped" version="0.1.1" />
            <dependency id="TypeScript.Compile" version="1.1.0" />
        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System" targetFramework="" />
        </frameworkAssemblies>
    </metadata>
    <files>
        <file src="..\Weld.Console\bin\Debug\Weld.dll" target="lib\net40\Weld.dll" />
        <file src="tools\install.ps1" target="tools\install.ps1" />
        <file src="..\Weld.Console\bin\Debug\RazorEngine.dll" target="tools\RazorEngine.dll" />
        <file src="tools\uninstall.ps1" target="tools\uninstall.ps1" />
        <file src="..\Weld.Console\bin\Debug\Weld.Console.exe" target="tools\Weld.Console.exe" />
        <file src="..\Weld.Console\bin\Debug\Weld.dll" target="tools\Weld.dll" />
    </files>
</package>